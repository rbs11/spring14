#bin/usimm input/4channel.cfg input/trace_test > output/test_pwrdwn 4 &
for (( x=1; x<=64; x=x*2 ))
do
	bin/usimm input/4channel.cfg input/A input/A input/A input/A > output/pwrdwn/pwrdwn1_$x $x &
	bin/usimm input/4channel.cfg input/B input/B input/B input/B > output/pwrdwn/pwrdwn2_$x $x &
	bin/usimm input/4channel.cfg input/C input/C input/C input/C > output/pwrdwn/pwrdwn3_$x $x &
	bin/usimm input/4channel.cfg input/D input/D input/D input/D > output/pwrdwn/pwrdwn4_$x $x &
	bin/usimm input/4channel.cfg input/E input/E input/E input/E > output/pwrdwn/pwrdwn5_$x $x &
	bin/usimm input/4channel.cfg input/A input/B input/C input/D > output/pwrdwn/pwrdwn6_$x $x &
	bin/usimm input/4channel.cfg input/A input/B input/C input/E > output/pwrdwn/pwrdwn7_$x $x &
	bin/usimm input/4channel.cfg input/A input/B input/D input/E > output/pwrdwn/pwrdwn8_$x $x &
	bin/usimm input/4channel.cfg input/A input/C input/D input/E > output/pwrdwn/pwrdwn9_$x $x &
	bin/usimm input/4channel.cfg input/B input/C input/D input/E > output/pwrdwn/pwrdwn10_$x $x &
done
